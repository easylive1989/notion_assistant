{
    "name": "Notion Assistant",
    "version": "0.1",
    "manifest_version": 2,
    "browser_action" :{
        "default_popup": "index.html",
        "default_icon" : "icons/assistant.png"
    },
    "permissions": [
        "cookies",
        "*://*.notion.com/*"
    ],
    "background": {
        "persistent": true,
        "scripts": ["background.js"]
    },
    "icons": {
        "16": "icons/assistant.png"
    }
}